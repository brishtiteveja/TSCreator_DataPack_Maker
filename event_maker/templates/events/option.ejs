<div class="sublist-header">
  <div class="icon-btn sublist-cancel-btn"><span>Edit</span></div>
  <div>EVENT Options</div>
</div>

<div class="display-option-detail">
    <label>Age</label>
    <input type="number" name="eventAge" value="">
</div>
<br/>
<div class="display-option-detail">
    <label>Description</label>
    <input type="text" id="eventPopup" name="eventPopup" value="">
</div>
<br/>

<div class="display-options-detail">
   <div>Line Type</div>
</div>
<hr>

<div class="display-options-details">
  <form>
 	<input type="radio" id="<%= name %>" name="event-line-type" value="solid" checked="">
  	<label>solid</label> <br>
  	<input type="radio" id="<%= name %>"  name="event-line-type" value="dotted">
  	<label>dotted</label> <br>
  	<input type="radio" id="<%= name %>" name="event-line-type" value="dashed">
  	<label>dashed</label> <br>
 </form>
</div>

<div class="display-options-detail">
   <div>Event Type</div>
</div>
<hr>

<div class="display-options-details">
  <form>
 	<input type="radio" id="<%= name %>"  name="event-type" value="LAD" checked="">
  	<label>LAD</label><br>
  	<input type="radio" id="<%= name %>" name="event-type" value="FAD">
  	<label>FAD</label><br>
  	<input type="radio" id="<%= name %>" name="event-type" value="EVENT">
  	<label>EVENT</label><br>
 </form>
</div>

<br/>
<div class="display-option-detail">
    <label>Reference</label>
    <input type="text" id="eventReference" name="eventReference" value="">
</div>

<br/>

<div class="display-options-details">
  <div>Change Image</div>
</div>
<hr>

<div class="display-options-details">
  <div class="file-dropbox image-dropbox" >
    <span class="data-status">Input event Image</span>
  </div>
</div>
